APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

#remove the '#' below and set your database configuration
DB_CONNECTION=(your database connection here e.g., mysql)
 DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD= 

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"


#LISK BLOCKCHAIN CONFIGURATION

#Lisk Network Settings
LISK_NETWORK= #your Lisk network (e.g., mainnet, sepolia)
LISK_RPC_URL= #your Lisk RPC URL (e.g., https://rpc-sepolia.lisk.com)
LISK_CHAIN_ID= #your Lisk chain ID (e.g., 4202)
LISK_EXPLORER_URL=https://sepolia-blockscout.lisk.com/

#Your BayanihanCebu Platform Wallet
BAYANIHAN_CEBU_WALLET= #your BayanihanCebu platform wallet address (e.g., 0x...)

#Your Private Key (64 characters, NO 0x prefix!)
#Replace with YOUR actual private key from MetaMask
LISK_PRIVATE_KEY= #your private key (64 characters, no 0x prefix)

#Blockchain Status
BLOCKCHAIN_ENABLED=true

PAYMONGO_PUBLIC_KEY= #your PayMongo public key
PAYMONGO_SECRET_KEY= #your PayMongo secret key
PAYMONGO_WEBHOOK_SECRET= #your PayMongo webhook secret
PAYMONGO_WEBHOOK_URL= #your PayMongo webhook URL


#BLOCKCHAIN CONTRACT CONFIGURATION

CONTRACT_ADDRESS= #your deployed contract address (e.g., 0x...) 
CONTRACT_ABI_PATH= ./path/to/your/contract_abi.json
METAMASK_PRIVATE_KEY= #your MetaMask private key (64 characters, no 0x prefix)
SEPOLIA_RPC_URL= #your Sepolia RPC URL (e.g., https://sepolia.infura.io/v3/your-infura-project-id)

# DonationRecorder Contract (Physical Donations)
DONATION_RECORDER_CONTRACT_ADDRESS=  #your DonationRecorder contract address (e.g., 0x...)    
BLOCKCHAIN_ADMIN_WALLET=  #your blockchain admin wallet address (e.g., 0x...)
BLOCKCHAIN_ADMIN_PRIVATE_KEY=  #your blockchain admin private key (64 characters, no 0x prefix)
WEB3_PROVIDER_URL=  #your Web3 provider URL (e.g., https://rpc.sepolia-api.lisk.com/)

